<% provide(:title, 'Create Task') %>
<h1>タスク新規作成</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@task) do |f| %>
      
      <p>名称</p>
      <%= f.text_field :task_title %>
      <p>詳細</p>
      <%= f.text_area :details %>
      <%= f.submit "新規登録",class:"btn btn-primary btn-block btn-create-task" %>
    <% end %>
  </div>
</div>